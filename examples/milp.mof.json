{
    "description": "The problem: min{x | x + y >= 1, x ∈ [0, 1], y ∈ {0, 1}}",
    "version": {
        "major": 0,
        "minor": 7
    },
    "variables": [{
        "name": "x",
        "primal_start": 0.0
    }, {
        "name": "y",
        "primal_start": 1.0
    }],
    "objective": {
        "sense": "min",
        "function": {
            "type": "SingleVariable",
            "variable": "x"
        }
    },
    "constraints": [{
        "name": "x + y >= 1",
        "function": {
            "type": "ScalarAffineFunction",
            "terms": [{
                    "coefficient": 1,
                    "variable": "x"
                },
                {
                    "coefficient": 1,
                    "variable": "y"
                }
            ],
            "constant": 0
        },
        "set": {
            "type": "GreaterThan",
            "lower": 1
        }
    }, {
        "name": "x ∈ [0, 1]",
        "function": {
            "type": "SingleVariable",
            "variable": "x"
        },
        "set": {
            "type": "Interval",
            "lower": 0,
            "upper": 1
        }
    }, {
        "name": "y ∈ {0, 1}",
        "function": {
            "type": "SingleVariable",
            "variable": "y"
        },
        "set": {
            "type": "ZeroOne"
        }
    }]
}
